    private LocalDateTime creTs;

    @Column(name = "cre_usr_id", length = 32, nullable = false)
    private String creUsrId;

    @Column(name = "updt_ts")
    private LocalDateTime updtTs;

    @Column(name = "updt_usr_id", length = 32)
    private String updtUsrId;
}
